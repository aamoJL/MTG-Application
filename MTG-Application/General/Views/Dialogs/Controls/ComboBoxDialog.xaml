<?xml version="1.0" encoding="utf-8"?>
<local:StringDialog
  x:Class="MTGApplication.General.Views.Dialogs.Controls.ComboBoxDialog"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:local="using:MTGApplication.General.Views.Dialogs.Controls"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d"
  Style="{StaticResource DefaultContentDialogStyle}">

  <ComboBox 
    Header="{x:Bind InputHeader, Mode=OneTime}"
    ItemsSource="{x:Bind Items, Mode=OneTime}"
    SelectedItem="{x:Bind Selection, Mode=TwoWay}"/>
</local:StringDialog>
